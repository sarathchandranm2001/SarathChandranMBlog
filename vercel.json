{
    "version": 2,
    "builds": [
      {
        "src": "my_site/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.12"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "my_site/wsgi.py"
      }
    ]
  }